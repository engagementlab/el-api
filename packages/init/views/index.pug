html
  head
    title= 'Engagement Lab CMS'

  body(style='background:#FAFBFB' onload='onload()')
  if noPermission
    h3 No permissions for app, select another

  div
    span Select an app
  
  if noAccess
    h3 You have no access to any app!
  else
    each val,index in apps
      div 
        a(href='/cms/@/' + index) #{val.name}